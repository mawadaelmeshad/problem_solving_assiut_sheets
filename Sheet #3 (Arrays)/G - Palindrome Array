#include <iostream>

using namespace std;

int main()
{

   long long  n;
    cin>>n;
    long long a[n];

    for (int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    long long j =n-1;
    for(int i=0;i<n;i++)
    {
        if (a[i]!= a[j])
        {
            cout<<"NO";
            return 0;

        }
        j--;
    }
    cout<<"YES";

    return 0;

}
