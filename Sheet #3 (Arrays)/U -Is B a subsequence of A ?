#include <iostream>

using namespace std;

int main()
{
   int a,b;  cin>>a>>b;
   int arr1[a],arr2[b];
   for(int i=0;i<a;i++)
   {
    cin>>arr1[i];
   }
   for(int i=0;i<b;i++)
   {
    cin>>arr2[i];
   }
   int j=0;
for(int i=0;i<a;i++)
  {
    if(arr2[j]==arr1[i])
    {
        j++;
    }
  }
  if(j==b)
  {
    cout<<"YES";
  }
  else
    cout<<"NO";

    return 0;
}
