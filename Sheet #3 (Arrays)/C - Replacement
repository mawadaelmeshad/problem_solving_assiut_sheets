#include <iostream>

using namespace std;

int main()
{
    int n;
    cin>>n;
    int k[n];
    for(int i=0;i<n;i++)
    {
      cin>>k[i];
    }
    for (int j=0;j<n;j++)
    {
        if (k[j]>0)
        {
            k[j]=1;
            cout<<k[j]<<" ";
        }
       else if (k[j]<0)
        {
            k[j]=2;
            cout<<k[j]<<" ";
        }
       else  if (k[j]==0)
        {
            k[j]=0;
            cout<<k[j]<<" ";
        }

    }
    return 0;
}
