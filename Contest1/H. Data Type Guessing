#include <iostream>
using namespace std;

int main()
{
 double n,k,a;  cin>>n>>k>>a;
double res= (n*k)/a;
long long result = res;
double fraction = res - result;
if(fraction > 0){
    cout<<"double";
}
else if(res <= 2147483647){
        cout<<"int";
    }
  else{
     cout<<"long long";
  }
    return 0;
}
