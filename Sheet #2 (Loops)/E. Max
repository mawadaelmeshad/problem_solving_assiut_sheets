#include <iostream>
#include <cstring>
 
using std::cout; using std::cin;
using std::endl; using std::string;
 
using namespace std;
int main()
{
   int n;
   int mini= -1;
   int k;
   cin>>n;
   for(int i=1;i<=n;i++)
   {
       cin>>k;
   mini = max(mini,k);
 
   }
 cout<<mini<<endl;
    return 0;
}
