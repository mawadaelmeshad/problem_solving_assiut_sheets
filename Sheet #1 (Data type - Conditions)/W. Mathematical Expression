#include <iostream>
 
using namespace std;
 
int main()
{
    int a,b,c;
   char s;
   char Q ;
    cin>>a>>s>>b>>Q>>c;
    if(s=='+' && c ==(a+b))
    {
        cout<<"Yes";
    }
    else if(s=='+' && c !=(a+b))
    {
        cout<<a+b;
    }
     if(s=='-' && c ==(a-b))
    {
        cout<<"Yes";
    }
    else if(s=='-' && c !=(a-b))
    {
        cout<<a-b;
    }
 
     if(s=='*' && c ==(a*b))
    {
        cout<<"Yes";
    }
    else if(s=='*' && c !=(a*b))
    {
        cout<<a*b;
    }
    return 0;
}
