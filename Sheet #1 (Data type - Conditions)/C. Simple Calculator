#include <iostream>
 
using namespace std;
 
int main()
{
    long long a;
    long long b;
    cin>>a>>b;
    long long sum,mul,dif;
    sum= a+b;
    mul=a*b;
    dif=a-b;
    cout<<a<<" + "<<b<<" = "<<sum<<endl;
    cout<<a<<" * "<<b<<" = "<<mul<<endl;
    cout<<a<<" - "<<b<<" = "<<dif<<endl;
    return 0;
}
