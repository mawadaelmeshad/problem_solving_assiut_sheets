#include <iostream>
#include <string>
#include <bits/stdc++.h>
using namespace std;

int main()
{
    int n;  cin>>n;
    while(n--)
    {
        string s,t;  cin>>s>>t;
        int maxi= max(s.length(),t.length());
        for(int i=0;i<maxi;i++)
        {
          if(i<s.length())
            cout<<s[i];
          if(i<t.length())
            cout<<t[i];
        }
        cout<<endl;
    }

    return 0;
}
